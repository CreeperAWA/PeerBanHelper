[Unit]
Description=PeerBanHelper Service
After=network.target
[Service]
User=peerbanhelper
WorkingDirectory=/etc/peerbanhelper
ExecStart=/usr/bin/java -Dpbh.datadir=/etc/peerbanhelper -Xmx512M -XX:+UseG1GC -XX:+UseStringDeduplication -XX:+ShrinkHeapInSteps -cp /usr/lib/peerbanhelper/libraries/* com.ghostchu.peerbanhelper.MainJumpLoader
Restart=on-failure
[Install]
WantedBy=multi-user.target
   
